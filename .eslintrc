{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint",
    "prettier"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "rules": {
    "no-console": "off",
    "prettier/prettier": "error",
    "@typescript-eslint/no-explicit-any": "off"
  },
  "ignorePatterns": [
    "*.json",
    "dist/**/*",
    "node_modules/**/*"
  ]
}