{
  "name": "ao-deploy",
  "type": "module",
  "version": "0.9.2",
  "packageManager": "pnpm@8.15.3",
  "description": "A package for deploying AO contracts. Supports both Node.js and web environments.",
  "author": "Pawan Paudel <pawanpaudel93@gmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/pawanpaudel93",
  "homepage": "https://github.com/pawanpaudel93/ao-deploy#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pawanpaudel93/ao-deploy.git"
  },
  "bugs": "https://github.com/pawanpaudel93/ao-deploy/issues",
  "keywords": [
    "ao",
    "arweave",
    "deployment",
    "web3",
    "blockchain",
    "smart-contracts",
    "web",
    "nodejs"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "browser": {
        "types": "./dist/index.web.d.ts",
        "import": "./dist/index.web.mjs"
      },
      "webpack": {
        "types": "./dist/index.web.d.ts",
        "import": "./dist/index.web.mjs"
      },
      "node": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.mjs",
        "require": "./dist/index.cjs"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.mjs",
        "require": "./dist/index.cjs"
      }
    },
    "./node": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./web": {
      "types": "./dist/index.web.d.ts",
      "import": "./dist/index.web.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "browser": "./dist/index.web.mjs",
  "typesVersions": {
    "*": {
      "web": [
        "./dist/index.web.d.ts"
      ],
      ".": [
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": {
    "aod": "./dist/cli.mjs",
    "ao-deploy": "./dist/cli.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild && tsx scripts/embedVersion",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "pnpm build",
    "release": "bumpp && npm publish",
    "release:beta": "bumpp --tag beta && npm publish --tag beta",
    "start": "esno src/index.ts",
    "test": "vitest",
    "test:run": "vitest --run",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks",
    "prepack": "pnpm build",
    "fmt": "prettier --write .",
    "fmt:check": "prettier --check ."
  },
  "dependencies": {
    "@permaweb/aoconnect": "^0.0.90",
    "arweave": "^1.15.7",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "jiti": "^1.21.6",
    "p-limit": "^4.0.0",
    "pretty-file-tree": "^1.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@types/node": "^20.17.9",
    "bumpp": "^9.8.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "esno": "^4.8.0",
    "globals": "^15.12.0",
    "lint-staged": "^15.2.10",
    "lua-format": "^1.5.2",
    "pnpm": "^8.15.9",
    "prettier": "^3.4.1",
    "rimraf": "^5.0.10",
    "simple-git-hooks": "^2.11.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.16.0",
    "unbuild": "^2.0.0",
    "vite": "^5.4.11",
    "vitest": "^1.6.0",
    "wao": "^0.35.6"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint . --fix"
  },
  "peerDependencies": {
    "lua-format": "^1.5.2"
  }
}
